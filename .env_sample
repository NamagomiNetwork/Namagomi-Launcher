# RESOURCES
GRAPH_ME_ENDPOINT=v1.0/me
GRAPH_MAIL_ENDPOINT=v1.0/me/messages

# Mac
APPLEID=<your@appleid>
APPLEIDPASS=<your-appleid-pass>
ASC_PROVIDER=<YOUR_ASC_PROVIDER>